{
  "expo": {
    "name": "echo-app",
    "slug": "echo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "echoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.echo.app",
      "config": {
        "googleMapsApiKey": "AIzaSyBvWnoXd8SmqKGrXP8yL7wag_13sVnUPcM"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.echo.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBvWnoXd8SmqKGrXP8yL7wag_13sVnUPcM"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "ECHO needs access to your camera to take photos for jobs.",
          "microphonePermission": false,
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "ECHO needs your location to verify you are at the job site.",
          "locationAlwaysPermission": "ECHO needs your location to find nearby jobs.",
          "locationWhenInUsePermission": "ECHO needs your location to find nearby jobs and verify photo locations."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "18754202-0db3-49cf-9707-23b2cf6fa1da"
      }
    },
    "owner": "sudosergio"
  }
}
